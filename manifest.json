{
    "manifest_version": 3,
    "name": "Kaskin",
    "description": "Insert purposefully written AI prompted text.",
    "version": "1.0.0",
    "action": {
        "default_popup": "loggedin.html"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["content.js"]
        }
      ]
  }
  